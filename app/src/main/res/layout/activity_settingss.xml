                android:layout_height="match_parent">
            <RelativeLayout
                android:id="@+id/rl_setting_parental"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <RelativeLayout
                    android:id="@+id/rl_player"
                    android:focusable="true"
                    android:paddingTop="5dp"
                    android:nextFocusDown="@+id/rl_parental"
                    android:background="@drawable/selector_checkbox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/tv_player_selection"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:background="@drawable/selector_parental_setting"
                        android:drawableLeft="@drawable/player_selection_icon"
                        android:drawablePadding="30dp"
                        android:fontFamily="@string/font_family_regular"
                        android:text="@string/player_setting"
                        android:textColor="@color/cart_list_item_color"
                        android:textSize="18dp"/>

                    <ImageView
                        android:id="@+id/iv_vod_forward_arrow"
                        android:layout_width="20dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:layout_centerInParent="true"
                        android:layout_marginEnd="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
